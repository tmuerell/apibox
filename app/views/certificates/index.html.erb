<h2>Certificates</h2>

<table class="table table-sm">
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @certificates.each do |certificate| %>
      <tr>
        <td><%= certificate.name %></td>
        <td><%= list_actions(certificate) %></td>
      </tr>
    <% end %>
  </tbody>
</table>